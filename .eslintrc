{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": [2, "global"],

    "default-case": 2,
    "no-self-compare": 2,
    "no-else-return": 2,
    "no-throw-literal": 2,
    "no-void": 2,
    "no-var": 2,
    "no-new-require": 2,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-multiple-empty-lines": [2, { "max": 1 }],
    "no-unused-vars": [2, {"args": "all", "argsIgnorePattern": "^_"}],
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "quotes": [2, "single"]
  }
}
